{
  "compilerOptions": {
    "outDir": "lib/",
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "jsx": "react",
    "lib": ["ESNext", "DOM"],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "pretty": true,
    "removeComments": false,
    "sourceMap": false,
    "strictPropertyInitialization": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": ["src"]
}
